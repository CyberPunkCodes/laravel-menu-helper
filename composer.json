    "keywords": ["laravel", "package", "menu", "nav", "navbar", "nav menu", "navigation", "dynamic", "helper"],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "CyberPunkCodes"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CyberPunkCodes\\MenuHelper\\": "src/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "require-dev": {},
    "extra": {
        "laravel": {
            "providers": [
                "CyberPunkCodes\\MenuHelper\\Providers\\MenuHelperServiceProvider"
            ]
        }
    }
}
